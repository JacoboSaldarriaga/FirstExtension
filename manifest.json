{
  "name": "First extension",
  "description": "Manifest for my first extension",
  "version": "1.0",
  "manifest_version": 3,
  "author": "Jacobo Saldarriaga",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/16Extension.png",
      "32": "/images/32Extension.png",
      "48": "/images/48Extension.png",
      "128": "/images/128Extension.png"
    }
  },
  "icons": {
    "16": "/images/16Extension.png",
    "32": "/images/32Extension.png",
    "48": "/images/48Extension.png",
    "128": "/images/128Extension.png"
  }
}
